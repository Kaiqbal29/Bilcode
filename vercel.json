    {
      "functions": {
        "api/**": {
          "includeFiles": "public/**",
          "maxDuration": 10
        }
      },
      "rewrites": [
        {
          "source": "/(.*)",
          "destination": "/index.php"
        }
      ],
       "routes": [
         {
           "src": "/(.*)",
           "dest": "public/$1"
         }
       ],
      "builds": [
        {
          "src": "index.php",
          "use": "@vercel/php"
        }
      ],
       "installCommand": "composer install && npm install",
       "buildCommand": "npm run prod && cp -r public/.vercel/output/static",
        "outputDirectory": ".vercel/output/static"
    }